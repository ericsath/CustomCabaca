<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eric.app.cabaca">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".home.book.bookdetail.BookDetail"></activity>
        <activity android:name=".home.writer.writerDetail.writerDetailMore.WriterDetailMore" />
        <activity android:name=".home.writer.writerDetail.WriterDetailActivity" />
        <activity android:name=".home.genre.bookgenre.BookGenre" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>